source('./<%= problemName %>.R')
suppressPackageStartupMessages({ require(testthat) })
<% for (var i = 0; i < cases.length; i++) { %>
test_that("<%= cases[i].description %>", {
  input <- "<%= cases[i].input %>"
  expect_equal(<%= funcName %>(input),
    "<%= cases[i].expected %>"
  )
})
<% } %>
print("All tests passed!")
